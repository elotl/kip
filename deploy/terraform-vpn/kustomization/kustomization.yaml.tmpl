bases:
- ../../manifests/kip/overlays/minikube/
namespace: kube-system
resources:
- vpn-deployment.yaml
- node-local-dns.yaml
patchesJson6902:
- target:
    group: apps
    version: v1
    kind: StatefulSet
    name: provider
  path: command-extra-args.yaml
configMapGenerator:
- name: config
  behavior: merge
  files:
  - provider.yaml
- name: aws-vpn-client-config
  envs:
  -  aws-vpn-client.env
secretGenerator:
- name: provider-secret
  literals:
  - AWS_ACCESS_KEY_ID=${aws_access_key_id}
  - AWS_SECRET_ACCESS_KEY=${aws_secret_access_key}
